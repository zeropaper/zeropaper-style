{
  "name": "zeropaper-style",
  "private": true,
  "version": "1.0.0-alpha-1",
  "config": {
    "prettierTarget": "{components,contexts,utils,pages,content,.storybook,.github}{/**/,/}*.{md,mdx,json,js,css,html,js,ts,tsx}"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "preview": "npm run preview:tina",
    "build": "run-s build:tina build:storybook",
    "prestart": "npm run build",
    "start": "next start",
    "lint": "next lint",
    "test": "run-p test:unit",
    "test:unit": "jest --passWithNoTests",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "test:e2e": "cypress run",
    "dev:test:e2e": "cypress open",
    "dev:storybook": "start-storybook -p 6006 --no-open",
    "dev:tina": "tinacms dev -c \"next dev\"",
    "preview:tina": "tinacms dev -c \"next build\"",
    "build:storybook": "build-storybook -o public/sb",
    "build:tina": "tinacms build && next build",
    "postbuild:tina": "next-sitemap"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@mantine/core": "5.2.4",
    "@mantine/hooks": "5.2.4",
    "@mantine/next": "5.2.4",
    "@tabler/icons": "1.91.0",
    "axios": "0.27.2",
    "next": "12.2.5",
    "next-mdx-remote": "4.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "three": "0.143.0",
    "tinacms": "0.69.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@emotion/server": "11.10.0",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@svgr/webpack": "6.3.1",
    "@swc/jest": "0.2.22",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@tinacms/cli": "0.61.7",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.14",
    "@types/react": "18.0.17",
    "@types/three": "0.143.2",
    "babel-loader": "8.2.5",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-storybook": "0.6.4",
    "jest": "29.0.1",
    "jest-environment-jsdom": "29.0.1",
    "next-sitemap": "3.1.21",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prosemirror-tables": "^1.2.5",
    "storybook-addon-next": "1.6.7",
    "storybook-css-modules-preset": "1.1.1",
    "styled-components": "^5.3.5",
    "typescript": "4.8.2",
    "webpack": "5.74.0",
    "yaml-loader": "0.8.0"
  }
}
