{
  "name": "zeropaper-style",
  "private": true,
  "version": "1.0.0-alpha-1",
  "config": {
    "prettierTarget": "{components,contexts,utils,pages,content,.storybook,.github}{/**/,/}*.{md,mdx,json,js,css,html,js,ts,tsx}"
  },
  "scripts": {
    "dev": "run-p start:*",
    "build": "run-s build:tina build:storybook",
    "start": "run-p start:*",
    "lint": "next lint",
    "start:storybook": "start-storybook -p 6006 --no-open",
    "build:storybook": "build-storybook -o public/sb",
    "test": "run-p test:unit",
    "test:unit": "jest --passWithNoTests",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "test:e2e": "cypress run",
    "start:test:e2e": "cypress open",
    "start:tina": "npx tinacms server:start -c \"next dev\"",
    "build:tina": "npx tinacms server:start -c \"next build\"",
    "postbuild:tina": "next-sitemap"
  },
  "dependencies": {
    "@mantine/core": "5.2.3",
    "@mantine/hooks": "5.2.3",
    "@mantine/next": "5.2.3",
    "@tabler/icons": "1.85.0",
    "@tinacms/auth": "^0.50.2",
    "@tinacms/cli": "^0.60.11",
    "axios": "0.27.2",
    "next": "12.1.4",
    "next-mdx-remote": "^4.0.2",
    "next-tinacms-cloudinary": "^3.5.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-tinacms-editor": "^0.53.10",
    "three": "^0.139.2",
    "tinacms": "^0.67.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@storybook/testing-library": "^0.0.9",
    "@svgr/webpack": "^6.2.1",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/three": "^0.139.0",
    "babel-loader": "^8.2.4",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-storybook": "^0.5.7",
    "jest": "^27.5.1",
    "next-sitemap": "^2.5.17",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "storybook-addon-next": "^1.6.2",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "prosemirror-tables": "^1.1.1",
    "styled-components": "^5.3.5",
    "webpack": "^5.71.0"
  }
}