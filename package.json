{
  "name": "zeropaper-style",
  "private": true,
  "version": "1.0.0-alpha-1",
  "config": {
    "prettierTarget": "{components,contexts,utils,pages,content,.storybook,.github}{/**/,/}*.{md,mdx,json,js,css,html,js,ts,tsx}"
  },
  "scripts": {
    "dev": "run-p dev:next dev:storybook",
    "build": "run-s build:next build:storybook",
    "prestart": "npm run build",
    "start": "next start",
    "lint": "next lint",
    "test": "run-p test:unit",
    "test:unit": "jest --passWithNoTests",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "test:e2e": "cypress run",
    "dev:test:e2e": "cypress open",
    "dev:storybook": "start-storybook -p 6006 --no-open",
    "dev:next": "tinacms dev -c \"next dev\"",
    "build:storybook": "build-storybook -o public/sb",
    "build:next": "tinacms build && next build",
    "postbuild:next": "next-sitemap"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@mantine/core": "5.10.0",
    "@mantine/hooks": "5.10.0",
    "@mantine/next": "5.10.0",
    "@tabler/icons": "1.119.0",
    "@tinacms/cli": "^1.0.5",
    "axios": "1.2.2",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "three": "0.148.0",
    "tinacms": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@emotion/server": "11.10.0",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-interactions": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/builder-webpack5": "6.5.15",
    "@storybook/manager-webpack5": "6.5.15",
    "@storybook/react": "6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@svgr/webpack": "6.5.1",
    "@swc/jest": "0.2.24",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/three": "0.148.0",
    "babel-loader": "9.1.2",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-storybook": "0.6.8",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "^13.1.0",
    "next-sitemap": "3.1.44",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.2",
    "prosemirror-tables": "^1.3.1",
    "storybook-addon-next": "1.7.1",
    "storybook-css-modules-preset": "1.1.1",
    "styled-components": "^5.3.6",
    "typescript": "4.9.4",
    "webpack": "5.76.0",
    "yaml-loader": "0.8.0"
  }
}
