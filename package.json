{
  "name": "zeropaper-style",
  "private": true,
  "version": "1.0.0-alpha-1",
  "config": {
    "prettierTarget": "{components,contexts,utils,pages,content,.storybook,.github}{/**/,/}*.{md,mdx,json,js,css,html,js,ts,tsx}"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "build": "run-p build:*",
    "start": "run-p start:*",
    "lint": "next lint",
    "start:storybook": "start-storybook -p 6006 --no-open",
    "build:storybook": "build-storybook",
    "test": "run-p test:*",
    "test:unit": "jest --passWithNoTests",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "test:e2e": "cypress run",
    "start:test:e2e": "cypress open",
    "start:tina": "npx tinacms server:start -c \"next dev\"",
    "build:tina": "npx tinacms server:start -c \"next build\"",
    "postbuild:tina": "next-sitemap"
  },
  "dependencies": {
    "@mantine/core": "^3.6.9",
    "@mantine/hooks": "^3.6.11",
    "@mantine/next": "^3.6.9",
    "@ory/integrations": "0.2.2",
    "@ory/kratos-client": "0.8.0-alpha.2",
    "@tinacms/auth": "^0.50.2",
    "@tinacms/cli": "^0.60.5",
    "classnames": "^2.3.1",
    "modern-css-reset": "^1.4.0",
    "next": "12.1.0",
    "next-mdx-remote": "^4.0.0",
    "next-tinacms-cloudinary": "^3.5.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-tinacms-editor": "^0.53.5",
    "tinacms": "^0.66.4",
    "tinycolor2": "^1.4.2",
    "typeface-bungee-inline": "^1.1.13",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@svgr/webpack": "^6.2.1",
    "@swc/jest": "^0.2.17",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@types/tinycolor2": "^1.4.3",
    "babel-loader": "^8.2.3",
    "cypress": "^8.6.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-storybook": "^0.5.7",
    "jest": "^27.5.1",
    "next-sitemap": "^2.4.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "storybook-addon-next": "^1.6.1",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "prosemirror-tables": "^1.1.1",
    "styled-components": "^5.3.3",
    "webpack": "^5.70.0"
  }
}